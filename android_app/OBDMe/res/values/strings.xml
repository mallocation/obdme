	<!-- Setup Wizard Complete Activity -->
	<string name="setupwizard.complete.label">Setup Complete</string>
	<string name="setupwizard.complete.heading.text">Setup Complete</string>
	<string name="setupwizard.complete.body.text">That\'s it! You are now ready to start using OBDMe.
