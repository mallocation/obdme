	<!-- Setup Wizard Main Activity -->
	<string name="setupwizard.main.label">OBDMe Setup Wizard</string>
	<string name="setupwizard.main.heading.text">Welcome to OBDMe!</string>
	<string name="setupwizard.main.text">It appears that this is the first time that you are
		starting OBDMe.\n\n
		This setup wizard will guide you through the
		processes of creating an
		OBDMe account and configuring your OBD dongle.
	</string>
	<string name="setupwizard.main.button.next.text">Next</string>

