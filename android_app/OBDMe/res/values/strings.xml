	<!-- Setup Wizard Bluetooth Activity -->
	<string name="setupwizard.bluetooth.label">Bluetooth Setup</string>
	<string name="setupwizard.bluetooth.heading.text">Bluetooth Setup</string>
	<string name="setupwizard.bluetooth.text.check">This screen will guide you through the Bluetooth
		setup process. Use the button below to check for Bluetooth support.
	</string>
	<string name="setupwizard.bluetooth.text.enable">Bluetooth is supported by your device. Use the
		button below to check if Bluetooth is enabled.</string>
	<string name="setupwizard.bluetooth.text.select">Bluetooth is currently enabled. Use the button
		below to select your OBDMe device.</string>
