		</pid>

		<pid hex="3F" return="2" unit="C" eval="FORMULA" pollable="true"
			name="Catalyst Temperature Bank 2, Sensor 2">
			<formula value="( ( ( ( A * 256 ) + B ) / 10 ) - 40 )" />
		</pid>

		<!-- MODE 1 PIDS 0x41-0x60 -->
