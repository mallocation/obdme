			name="PIDs supported 41-60">
			<code bit="0" value="41" />
			<code bit="1" value="42" />
			<code bit="2" value="43" />
			<code bit="3" value="44" />
			<code bit="4" value="45" />
			<code bit="5" value="46" />
			<code bit="6" value="47" />
			<code bit="7" value="48" />
